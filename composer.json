{
  "name": "8fold/php-html-spec",
  "description": "The HTML and ARIA-HTML specifications as structured data",
  "type": "library",
  "license": "MIT",
  "authors": [],
  "require": {
    "php": "~7|^8.0",
    "illuminate/support": "~8.0",
    "8fold/php-shoop": "~0.10"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.1"
  },
  "autoload": {
    "psr-4": {
      "Eightfold\\HtmlSpec\\":"./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eightfold\\HtmlSpec\\Tests\\":"./tests"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true
}
